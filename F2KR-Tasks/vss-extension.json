{
    "manifestVersion": 1,
    "id": "F2KR-build-extension",
    "name": "Script Builder",
    "version": "0.0.2",
    "publisher": "Farzad",
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "public": true,
    "description": "building script based on git branch name (support pattern) e.g. Dockerfile , kubernetes yaml file (including Blue/Greendeployment) " ,
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [{
            "path": "F2KR-Task1"
        },
        {
            "path": "F2KR-Task2"
        },
        {
            "path": "Template1"
        }
    ],
    "contributions": [{
            "id": "F2KR-Task1",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "F2KR-Task1"
            }
        },
        {
            "id": "F2KR-Task2",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "F2KR-Task2"
            }
        }
    ]
}