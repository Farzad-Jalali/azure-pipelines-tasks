{
    "manifestVersion": 1,
    "id": "F2KR-build-extension",
    "name": "Script Builder",
    "version": "0.0.4",
    "publisher": "Farzad",
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "public": true,
    "description": "building script based on git branch name (support pattern) e.g. Dockerfile , kubernetes yaml file (including Blue/Greendeployment) " ,
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "Extension",
        "Marketplace",
        "Publish",
        "Package",
        "Install",
        "Continuous Integration",
        "Continuous Delivery",
        "Build",
        "Release",
        "Azure Pipelines",
        "Azure DevOps Extensions",
        "Visual Studio Extensions"
      ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [{
            "path": "F2KR-Task1"
        },
        {
            "path": "F2KR-Task2"
        },
        {
            "path": "Resources"
        }
    ],
    "content": {
        "details": {
          "path": "Resources/overview.md"
        },
        "license": {
          "path": "Resources/license.txt"
        }
      },
    "contributions": [{
            "id": "F2KR-Task1",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "F2KR-Task1"
            }
        },
        {
            "id": "F2KR-Task2",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "F2KR-Task2"
            }
        }
    ],
    "links": {
        "repository": {
          "uri": "https://github.com/Microsoft/azure-devops-extension-tasks"
        },
        "issues": {
          "uri": "https://github.com/Microsoft/azure-devops-extension-tasks/issues"
        },
        "support": {
          "uri": "mailto:azdevopsintegrations@microsoft.com"
        }
      },
      "repository": {
        "type": "git",
        "uri": "https://github.com/Microsoft/azure-devops-extension-tasks"
      }
}